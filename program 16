#include <stdio.h>

int main() {
    int i, j, n, f, pages[50], frames[10], index = 0, faults = 0;
    
    printf("Enter number of pages: ");
    scanf("%d", &n);
    printf("Enter page reference string: ");
    for(i = 0; i < n; i++)
        scanf("%d", &pages[i]);
    
    printf("Enter number of frames: ");
    scanf("%d", &f);
    
    for(i = 0; i < f; i++)
        frames[i] = -1;

    for(i = 0; i < n; i++) {
        int flag = 0;
        for(j = 0; j < f; j++)
            if(frames[j] == pages[i])
                flag = 1;

        if(flag == 0) {
            frames[index] = pages[i];
            index = (index + 1) % f;
            faults++;
        }

        printf("\nFrames: ");
        for(j = 0; j < f; j++)
            printf("%d ", frames[j]);
    }

    printf("\nTotal Page Faults = %d\n", faults);
    return 0;
}
